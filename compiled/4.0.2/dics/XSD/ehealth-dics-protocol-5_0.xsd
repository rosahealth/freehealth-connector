			<xs:element name="Text" minOccurs="2" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>At least two translations (dutch and french) are required for a multi-language text field.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute ref="xml:lang" use="required"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ConsultVirtualIngredientType">
		<xs:sequence>
			<xs:element name="Type" type="xs:string">
				<xs:annotation>
					<xs:documentation>Type of use for the specified substance (given by the link to the reference entity Substance).
                Possible values :
                - ACTIVE_SUBSTANCE: the substance participates in the medicinal product effect ;
                - EXCIPIENT: not active substance used to make the medicinal product more attractive.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Strength" type="StrengthRangeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Strength of the specified substance. Some substance quantities cannot be specified exactly, so a range is provided. If an exact quantity is known, minimum and maximum will have the same value.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Substance" type="SubstanceWithStandardsType"/>
			<xs:element name="AdditionalFields" type="AdditionalFieldsType" minOccurs="0" maxOccurs="unbounded">
