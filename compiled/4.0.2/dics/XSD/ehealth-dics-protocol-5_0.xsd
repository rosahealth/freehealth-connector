				</xs:annotation>
			</xs:element>
			<xs:element name="TreatmentDurationType" type="xs:string">
				<xs:annotation>
					<xs:documentation>Treatment duration type. Values: ONE_OFF, TEMPORARY, CHRONIC, IF_NECESSARY</xs:documentation>
				</xs:annotation>
			</xs:element>
